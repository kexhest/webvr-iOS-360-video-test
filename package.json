{
  "name": "genesis-three",
  "version": "1.0.0",
  "description": "Boilerplate with threejs, webpack, babel and lots of goodies.",
  "author": "Magnus Bergman <hello@magnus.sexy>",
  "engines": {
    "node": "5.1.1",
    "npm": "3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:magnus-bergman/genesis-three.git"
  },
  "scripts": {
    "postinstall": "npm run build",
    "test": "npm run lint & cross-env NODE_ENV=test karma start",
